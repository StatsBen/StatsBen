{
  "name": "adventures-v2",
  "version": "3.0.6",
  "description": "My personal website",
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules",
      "/demo-entries/"
    ]
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/StatsBen/statsben.git"
  },
  "scripts": {
    "test": "jest --silent",
    "test-coverage": "jest --silent --coverage",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "dev": "parcel src/index.html",
    "build": "parcel build src/index.html",
    "deploy": "npm version patch && git push && parcel build src/index.html && firebase use --add bendclark-456 && firebase deploy --only hosting",
    "deploy:functions": "firebase use --add adventures-41a0 && firebase deploy --only functions"
  },
  "keywords": [],
  "author": "Ben Clark",
  "license": "ISC",
  "dependencies": {
    "@emotion/core": "^10.0.35",
    "@reach/router": "^1.3.4",
    "eslint-plugin-react-hooks": "^4.1.0",
    "firebase": "^7.19.1",
    "firebase-admin": "^9.1.1",
    "firebase-functions": "^3.11.0",
    "moment": "^2.27.0",
    "parcel-bundler": "^1.12.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-favicon": "0.0.17",
    "react-loadable": "^5.5.0",
    "react-select": "^3.1.0",
    "reselect": "^4.0.0",
    "styled": "^1.0.0",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "jest": "^26.4.2",
    "npm-version": "^1.1.0",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.13.1"
  }
}
